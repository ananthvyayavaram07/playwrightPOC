{
  "name": "playwright-framework",
  "version": "1.0.0",
  "description": "Playwright TypeScript Framework with Playwright Test Runner",
  "scripts": {
    "postinstall": "npx playwright install",
    "qa": "cross-env test_env=qa npx playwright test --workers=1",
    "staging": "cross-env test_env=staging npx playwright test --workers=1"
  },
  "keywords": [
    "playwright test runner",
    "playwright typescript framework"
  ],
  "author": "Ananth",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40",
    "@types/node": "^20.10.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "xlsx": "^0.18.5"
  }
}
